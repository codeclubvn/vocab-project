{
  "name": "next-js-boilerplate",
  "version": "3.15.0",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "build-stats": "cross-env ANALYZE=true npm run build",
    "build-prod": "run-s clean build",
    "clean": "rimraf .next .swc out coverage",
    "lint": "next lint",
    "commit": "cz",
    "storybook": "storybook dev -p 6006",
    "storybook:build": "storybook build",
    "storybook:serve": "http-server storybook-static --port 6006 --silent",
    "serve-storybook": "run-s storybook:*",
    "test-storybook:ci": "start-server-and-test serve-storybook http://localhost:6006 test-storybook",
    "prepare": "cd .. && husky install FrontEnd/.husky",
    "postbuild": "next-sitemap",
    "lint:fix": "eslint --fix src --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\""
  },
  "dependencies": {
    "@dnd-kit/core": "^6.0.8",
    "@dnd-kit/sortable": "^7.0.2",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/react-navigation-menu": "^1.1.4",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/themes": "^1.1.2",
    "@tanstack/react-query": "^4.33.0",
    "axios": "^1.5.0",
    "encoding": "^0.1.13",
    "firebase": "^10.4.0",
    "formik": "^2.4.5",
    "js-cookie": "^3.0.5",
    "next": "^13.4.19",
    "next-seo": "^6.1.0",
    "next-sitemap": "^4.1.8",
    "react": "^18.2.0",
    "react-card-flip": "^1.2.2",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-query": "^3.39.3",
    "react-swisskit": "^1.0.6",
    "swiper": "^10.3.1",
    "usehooks-ts": "^2.9.1",
    "yup": "^1.3.2",
    "zustand": "^4.4.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@commitlint/cz-commitlint": "^17.6.7",
    "@dwarvesf/react-eslint-config": "^1.0.1",
    "@next/bundle-analyzer": "^13.4.13",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@storybook/addon-essentials": "^7.0.27",
    "@storybook/addon-interactions": "^7.0.27",
    "@storybook/addon-links": "^7.0.27",
    "@storybook/blocks": "^7.0.27",
    "@storybook/nextjs": "^7.0.27",
    "@storybook/react": "^7.0.27",
    "@storybook/test-runner": "^0.11.0",
    "@storybook/testing-library": "^0.2.0",
    "@types/node": "^20.4.8",
    "@types/react": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.2.1",
    "@typescript-eslint/parser": "^6.2.1",
    "autoprefixer": "^10.4.14",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^6.0.1",
    "eslint": "^8.46.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-next": "^13.4.13",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "eslint-plugin-testing-library": "^6.1.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "http-server": "^14.1.1",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.27",
    "prettier": "^3.0.1",
    "rimraf": "^5.0.1",
    "semantic-release": "^19.0.5",
    "start-server-and-test": "^2.0.0",
    "storybook": "^7.0.27",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/changelog",
      [
        "@semantic-release/npm",
        {
          "npmPublish": false
        }
      ],
      "@semantic-release/git",
      "@semantic-release/github"
    ]
  },
  "author": "Ixartz (https://github.com/ixartz)"
}
